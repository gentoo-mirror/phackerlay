user=seafile
group=seafile

start() {
	/opt/seafile/seafile-server-latest/seafile.sh start
	/opt/seafile/seafile-server-latest/seahub.sh start
}

stop() {
	/opt/seafile/seafile-server-latest/seafile.sh stop
	/opt/seafile/seafile-server-latest/seahub.sh stop
}
